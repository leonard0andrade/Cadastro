    private void c_nomeActionPerformed(java.awt.event.ActionEvent evt) {                                       
        Usuario F = new Usuario();
        Usuario F2 = new Usuario();
        F.getNome();
        F2.getNome();
        setVisible(true);
        
        if(F.getNome() == c_nome.getText()){
            F.getNome();
            
            setVisible(true);
            
        
        } else if(F.getNome()!= F2.getNome()){
            JOptionPane.showMessageDialog(this, "Senha incorreta! tente novamente.");
            F.setNome("");
            F.setNome("");
        }
    }                                      

    private void c_entrarActionPerformed(java.awt.event.ActionEvent evt) {                                         
        new Principal().setVisible(true);
        this.dispose();
    }                                        

    private void CampodoCadastroActionPerformed(java.awt.event.ActionEvent evt) {                                                
    
    }                                               

    private void c_senhaActionPerformed(java.awt.event.ActionEvent evt) {                                        
        Usuario F = new Usuario();
        Usuario F2 = new Usuario();
        F.getSenha();
        F2.getSenha();
        setVisible(true);
        
        
        
        if(F.getSenha()== c_senha.getText() == true){
            
             System.out.println(c_entrar);
            
        
        } else if(F.getSenha()!= F2.getSenha()){
            JOptionPane.showMessageDialog(this, "Senha incorreta! tente novamente.");
            F.setSenha("");
            F.setSenha("");
        }
        
        
       
                   
    }                                       

